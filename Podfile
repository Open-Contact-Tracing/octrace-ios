platform :ios, '11.0'

# ignore all warnings from all pods
inhibit_all_warnings!

def pods
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for octrace
  pod 'Alamofire' # 5.1.0
  pod 'AlamofireNetworkActivityIndicator' # 3.1.0
